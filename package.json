{
  "name": "sandbox-css-in-js-with-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "install-precommit": "husky install",
    "lint": "run-p -l -c --aggregate-output lint:*",
    "lint:eslint": "next lint",
    "lint:prettier": "prettier --check .",
    "lint:stylelint": "stylelint **/*.ts{,x}",
    "fmt": "run-s fmt:prettier fmt:eslint fmt:stylelint",
    "fmt:eslint": "next lint --fix",
    "fmt:prettier": "prettier --write .",
    "fmt:stylelint": "stylelint --fix **/*.ts{,x}",
    "precommit": "run-p -l --aggregate-output type-check lint:*"
  },
  "dependencies": {
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@stylelint/postcss-css-in-js": "^0.37.2",
    "@types/node": "17.0.21",
    "@types/react": "17.0.41",
    "@types/styled-components": "^5.1.24",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "eslint": "8.11.0",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^7.0.4",
    "npm-run-all": "^4.1.5",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.6.0",
    "styled-components": "^5.3.5",
    "stylelint": "^14.8.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "typescript": "4.6.2"
  },
  "volta": {
    "node": "17.7.2",
    "npm": "8.5.5"
  }
}
